      <xs:element name="showTermsAndConditions" type="xs:boolean">
            
        <xs:annotation>
              
          <xs:documentation>TRUE, falls der Benutzer den aktuellen
AGB's zugestimmt hat, ansonst FALSE
							</xs:documentation>
            
        </xs:annotation>
          
      </xs:element>
          
      <xs:element minOccurs="0" name="infoMessage" type="xs:string"/>
        
