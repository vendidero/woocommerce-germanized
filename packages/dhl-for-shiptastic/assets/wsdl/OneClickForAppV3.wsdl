  <xs:simpleType name="VoucherLayout">
        
    <xs:restriction base="xs:string">
          
      <xs:enumeration value="AddressZone"/>
          
      <xs:enumeration value="FrankingZone"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="Flag">
        
    <xs:annotation>
          
      <xs:documentation>Ja/Nein-Entscheidung.</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:boolean"/>
      
  </xs:simpleType>
      
  <xs:simpleType name="ShippingList">
        
    <xs:annotation>
          
      <xs:documentation>Versandliste Flag mögliche Werte:	0 = keine Versandliste; 1 = Versandliste ohne Adressen;	2 = Versandliste mit Adressen</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:enumeration value="0"/>
          
      <xs:enumeration value="1"/>
          
      <xs:enumeration value="2"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="PageFormatId">
        
    <xs:annotation>
          
      <xs:documentation>ID des Druckformates</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:int"/>
      
  </xs:simpleType>
      
  <xs:simpleType name="paymentRecipient">
        
    <xs:annotation>
          
      <xs:documentation>Zahlungsempfänger</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:maxLength value="54"/>
          
      <xs:minLength value="1"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="paymentReference">
        
    <xs:annotation>
          
      <xs:documentation>Zahlungsreferenz</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:maxLength value="140"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
