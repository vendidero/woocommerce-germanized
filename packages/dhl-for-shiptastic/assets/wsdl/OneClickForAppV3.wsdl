      <xs:documentation>DPI der zu erstellenden Frankierlabel</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:int">
          
      <xs:enumeration value="300"/>
          
      <xs:enumeration value="203"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:complexType name="AuthenticateUserResponseType">
        
    <xs:annotation>
          
