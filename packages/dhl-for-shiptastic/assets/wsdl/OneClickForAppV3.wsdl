    <xs:annotation>
          
      <xs:documentation>IBAN des Zielkontos</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:pattern value="[a-zA-Z]{2}[0-9]{2}[0-9a-zA-Z]{11,30}"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="bic">
        
    <xs:annotation>
          
      <xs:documentation>BIC des Zielkontos</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:pattern value="[a-zA-Z]{4}[A-Z]{2}[a-zA-Z0-9]{2}([a-zA-Z0-9]{3})?"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="accountOwner">
        
    <xs:annotation>
          
      <xs:documentation>Inhaber des Zielkontos</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:string">
          
      <xs:maxLength value="54"/>
          
      <xs:minLength value="1"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:simpleType name="amount">
        
    <xs:annotation>
          
      <xs:documentation>Nachnahmebetrag in Eurocent</xs:documentation>
        
    </xs:annotation>
        
    <xs:restriction base="xs:int">
          
      <xs:maxInclusive value="160000"/>
          
      <xs:minExclusive value="0"/>
        
    </xs:restriction>
      
  </xs:simpleType>
      
  <xs:complexType name="AuthenticateUserRequestType">
        
    <xs:annotation>
          
      <xs:documentation>Struktur um einen Benutzeraccount zu verifizieren</xs:documentation>
        
    </xs:annotation>
        
